{
  "manifest_version": 3,
  "name": "Video Queue Saver",
  "version": "1.0.0",
  "description": "Guarda pestañas abiertas en una lista y expórtalas a CSV con un clic.",
  "permissions": ["tabs", "storage", "downloads"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Video Queue Saver"
  }
}
